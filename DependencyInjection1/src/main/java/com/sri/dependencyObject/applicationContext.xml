<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- <bean id="student" class="com.sri.model.Student" p:name="Srikanth" 
		p:id="308"/> -->
	<bean id="student" class="com.sri.dependencyObject.Student"
		p:id="308">
		<property name="mathCheat" ref="mathCheatObjectValue" />
	</bean>
	<bean id="anotherStudent"
		class="com.sri.dependencyObject.AnotherStudent" p:id="302">
		<property name="cheat" ref="mathCheatObjectValue" />
	</bean>
	<bean id="mathCheatObjectValue" class="com.sri.dependencyObject.MathCheat" />

</beans>