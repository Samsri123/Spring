<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="cus" class="com.sri.beans.Customer" p:name="Srikanth" p:contact="83418666451" p:address="Hindupur" />
	<bean id="ord" class="com.sri.beans.Order" p:productName="RedmiK20" p:productID="123" p:productPrice="200000">
		<property name="customer" ref="cus" />
	</bean>

</beans>